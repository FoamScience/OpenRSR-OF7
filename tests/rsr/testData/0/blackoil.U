FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      blackoil.U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }

    outlet
    {
        type            zeroGradient;
    }

    emptyWalls
    {
        type            empty;
    }
}

